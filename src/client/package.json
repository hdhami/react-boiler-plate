{
    "name": "boiler-plate-code",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "global:install:ci": "sudo npm i -g cross-env webpack-cli webpack npm-run-all husky",
        "global:install:local": "npm i -g cross-env webpack-cli webpack npm-run-all husky",
        "npm:install:ci": "npm install",
        "npm:install:local": "npm install && cd ../server && npm install",
        "serve": "cd ../server && npm run serve",
        "dev:client": "cross-env NODE_ENV=development webpack-dev-server --profile --progress --colors --config app_modules/compiler/webpack/webpack.config.babel.js",
        "dev:start": "npm-run-all --parallel dev:client serve",
        "build:prod": "cross-env NODE_ENV=production webpack --profile --progress --colors --config app_modules/compiler/webpack/webpack.config.babel.js",
        "generate:clientlibs:support": "cd app_modules/compiler/webpack && node clientlib-generator.js",
        "precommit": "lint-staged --config app_modules/hooks/husky/.lintstagedrc",
        "cosmics": "prettier --config app_modules/cosmics/prettier/.prettierrc --write \"app/**/*.{js,scss}\"",
        "lint:js": "eslint --config app_modules/linter/js-linter/.eslintrc app/**/*.js",
        "lint:style": "stylelint --config app_modules/linter/style-linter/.stylelintrc \"app/**/*.{scss}\"",
        "linter": "npm-run-all --parallel lint:js lint:style",
        "kill-node": "sh kill.node.sh"
    },
    "author": "Harish Singh Dhami",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.5.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@babel/register": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "cors": "^2.8.5",
        "cross-env": "^5.2.0",
        "editorconfig": "^0.15.2",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.6.0",
        "enzyme-to-json": "^3.3.4",
        "eslint": "^5.8.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-react": "^7.11.1",
        "file-loader": "^5.0.2",
        "fs-extra": "^7.0.1",
        "husky": "^1.3.1",
        "jest": "^23.6.0",
        "lint-staged": "^8.0.4",
        "mini-css-extract-plugin": "^0.5.0",
        "nodemon": "^2.0.2",
        "npm-run-all": "^4.1.5",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "pa11y": "^5.1.0",
        "pa11y-ci": "^2.1.1",
        "prettier": "1.15.3",
        "stylelint": "^9.7.1",
        "terser-webpack-plugin": "^2.3.1",
        "webpack-dev-server": "^3.10.1"
    },
    "dependencies": {
        "@material-ui/core": "^4.8.3",
        "axios": "^0.19.2",
        "glob": "^7.1.6",
        "prop-types": "^15.7.2",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "styled-components": "4.3.2",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-merge": "^4.2.2"
    }
}
